import java.awt.Color;

import javax.swing.JFrame;

public class GUI extends JFrame
{
	public GUI() 
	{
		
		this.setVisible(true); //Make visible
		this.setSize(420,420); //sets the x and y dimension of this.
		this.setTitle("Electrical Calculator"); // Sets title of window.
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE); // Closes window when hit x button.
		this.setResizable(false); //Prevent resizing of this.
		
		this.getContentPane().setBackground(new Color(176,227,255)); //Background color of window
	}
}
